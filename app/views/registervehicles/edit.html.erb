<h1>Update RegisterVehicles</h1>
<%= form_for Registervehicle.new do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, :value => Registervehicle.find(params[:id]).name %>
  </div>
  <div class="field">
    <%= f.label :address %>
    <%= f.text_field :address, :value => Registervehicle.find(params[:id]).address %>
  </div>
  <div class="field">
    <%= f.label :phonenumber %>
    <%= f.text_field :phonenumber, :value => Registervehicle.find(params[:id]).phonenumber %>
  </div>

  <div class="field">
    <%= f.label :vehiclenumber %>
    <%= f.text_field :vehiclenumber, :value => Registervehicle.find(params[:id]).phonenumber %>
  </div>

  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>